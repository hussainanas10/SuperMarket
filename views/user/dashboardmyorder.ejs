								<a href="/logout" class="user-item"><i class="uil uil-exit"></i>Logout</a>
							</div>
						</div>
					</div>
					<div class="col-lg-9 col-md-8">
						<div class="dashboard-right">
							<div class="row">
								<div class="col-md-12">
									<div class="main-title-tab">
										<h4><i class="uil uil-box"></i>My Orders</h4>
									</div>
								</div>
								<div class="col-lg-12 col-md-12">
									<%for(var i=0;i<order.items.length;i++) {%>
									<div class="pdpt-bg">
										 
										<div class="order-body10">
											<ul class="order-dtsll">
												<li>
													<div class="order-dt-img">
														<img src="/uploads/<%=order.items[i].productId.images[0]%>" alt="">
													</div>
												</li>
												<li>
													<div class="order-dt47">
														<h4><%=order.items[i].address%></h4>
														<p>Delivered -<%=settings.name%></p>
														<div class="order-title"><%=order.items[i].productId.productname%> <span data-inverted="" data-tooltip="<%=order.items[i].qty%>" data-position="top center">?</span></div>
													</div>
												</li>
											</ul>
											<div class="total-dt">
												<div class="total-checkout-group">
													<div class="cart-total-dil">
														<h4>Sub Total</h4>
														<span>Rs<%= order.items[i].qty*order.items[i].productId.discountmrp %></span>
													</div>
													<div class="cart-total-dil pt-3">
														<h4>Delivery Charges</h4>
														<span>Free</span>
													</div>
												</div>
												<div class="main-total-cart">
													<h2>Total</h2>
													<span>Rs<%= order.items[i].qty*order.items[i].productId.discountmrp %></span>
												</div>
											</div>
											
										</div>
									</div>
									<%}%>
									
								</div>								
							</div>
						</div>
					</div>
